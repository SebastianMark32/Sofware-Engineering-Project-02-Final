{
  "name": "angular-draggable-droppable",
  "version": "8.0.0",
  "description": "Drag and drop for angular 15.0+",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattlewis92/angular-draggable-droppable.git"
  },
  "keywords": [
    "angular2",
    "angular",
    "draggable",
    "droppable",
    "drag",
    "drop"
  ],
  "author": "Matt Lewis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mattlewis92/angular-draggable-droppable/issues"
  },
  "homepage": "https://github.com/mattlewis92/angular-draggable-droppable#readme",
  "peerDependencies": {
    "@angular/core": ">=15.0.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "@mattlewis92/dom-autoscroller": "^2.4.2",
    "tslib": "^2.4.1"
  },
  "packageManager": "pnpm@7.18.1",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@angular-devkit/*"
      ]
    }
  },
  "module": "fesm2015/angular-draggable-droppable.mjs",
  "es2020": "fesm2020/angular-draggable-droppable.mjs",
  "esm2020": "esm2020/angular-draggable-droppable.mjs",
  "fesm2020": "fesm2020/angular-draggable-droppable.mjs",
  "fesm2015": "fesm2015/angular-draggable-droppable.mjs",
  "typings": "index.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./index.d.ts",
      "esm2020": "./esm2020/angular-draggable-droppable.mjs",
      "es2020": "./fesm2020/angular-draggable-droppable.mjs",
      "es2015": "./fesm2015/angular-draggable-droppable.mjs",
      "node": "./fesm2015/angular-draggable-droppable.mjs",
      "default": "./fesm2020/angular-draggable-droppable.mjs"
    }
  },
  "sideEffects": false
}